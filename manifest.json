{
  "manifest_version": 2,
  "name": "Catify",
  "version": "1.0",
  "description": "ALL IMAGES ARE NOW MONITO",
  "background" : {
    "scripts": ["scripts/convertImagesToCat.js"],
    "persistent": true
  },
